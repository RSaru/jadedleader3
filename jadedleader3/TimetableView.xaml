<Window x:Class="jadedleader3.TimetableView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:jadedleader3"
        mc:Ignorable="d"
        Title="TimetableView" Height="900" Width="1700">
    <Grid>
        <DataGrid x:Name="DGDisplayLectures" CanUserAddRows="False" AutoGenerateColumns="False" HorizontalAlignment="Left" Height="566" Margin="10,71,0,0" VerticalAlignment="Top" Width="907" d:ItemsSource="{d:SampleData ItemCount=5}" SelectionChanged="DataGrid_SelectionChanged">
            <DataGrid.Columns>
                <DataGridTextColumn Header="9:00" Width="100" />
                <DataGridTextColumn Header="10:00" Width="100" />
                <DataGridTextColumn Header="11:00" Width="100" />
                <DataGridTextColumn Header="12:00" Width="100" />
                <DataGridTextColumn Header="13:00" Width="100" />
                <DataGridTextColumn Header="14:00" Width="100" />
                <DataGridTextColumn Header="15:00" Width="100" />
                <DataGridTextColumn Header="16:00" Width="100" />
                <DataGridTextColumn Header="17:00" Width="100" />
            </DataGrid.Columns>
        </DataGrid>
        <Image HorizontalAlignment="Left" Height="61" Margin="10,10,0,0" VerticalAlignment="Top" Width="238" Source="/logo.jpg"/>
    </Grid>
</Window>
